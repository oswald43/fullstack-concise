meta {
  name: get one job
  type: graphql
  seq: 2
}

post {
  url: {{baseUrl}}
  body: graphql
  auth: inherit
}

body:graphql {
  query GetJob($id: ID!) {
    job(id: $id) {
      id
      title
      date
    }
  }
  
}

